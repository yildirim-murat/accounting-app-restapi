pipeline {
  
  agent any
  
  stages {
    
    stage("Build") {
        echo 'build app'
      }
    
    stage("Test") {
      
      steps{
          echo 'testing the application codes'
      }
    }

    stage("Deploy") {
      
      steps{
          echo 'deploying the app'
      }
    }
  }
}
